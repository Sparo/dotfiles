# izdajnici i zapadni mediji
izdajnici=(
    http://blic.rs
    http://b92.net
)

# vesti iz Srbije i sveta
vesti=(
    http://srbin.info
    http://srbinaokup.info
    http://vaseljenska.com
    http://fakti.rs
    http://fsksrb.ru
    http://nspm.rs
)

